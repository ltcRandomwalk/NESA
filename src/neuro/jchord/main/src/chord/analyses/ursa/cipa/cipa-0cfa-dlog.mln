*objNewInstIH(i_dom, h_dom)
*MmethArg(m_dom, z_dom, v_dom)

*VHfilter(v_dom, h_dom) //this differs from the original dlog file, wher VHfilter is an idb relation

// non-input predicates, which can appear on the lhs of each rule
IM(i_dom, m_dom)
VV(v_dom, v_dom)
VH(v_dom, h_dom)
objVarAsgnInst(v_dom, v_dom)
objValAsgnInst(v_dom, h_dom)
rootM(m_dom)
VH_33_1(v_dom, h_dom)
MM(m_dom, m_dom)
reachableM(m_dom)
VH_33_0(v_dom, f_dom, h_dom)
putStatFldInst(f_dom, v_dom)
getInstFldInst(v_dom, v_dom, f_dom)
specIMV(i_dom, m_dom, v_dom)
getStatFldInst(v_dom, f_dom)
IHM(i_dom, h_dom, m_dom)
HFH_34_0(f_dom, v_dom, h_dom)
reachableT(t_dom)
FH(f_dom, h_dom)
HFH(h_dom, f_dom, h_dom)
reachableI(i_dom)
putInstFldInst(v_dom, f_dom, v_dom)

// hard rules converted from dlog into soft rules
//1 VT(v1,t), HT(h,s), sub(s,t) => VHfilter(v1,h)
//VHfilter(v1,0).
1 virtIM(i,m1), HT(h,t), cha(m1,t,m2) => IHM(i,h,m2)
reachableM(0).
1 reachableT(t), clinitTM(t,m) => reachableM(m)
1 IM(a,m) => reachableM(m)
1 MI(m,i), reachableM(m) => reachableI(i)
1 reachableI(i), IinvkArg0(i,v1), VH(v1,h), IHM(i,h,m) => IM(i,m)
1 specIM(i,m), IinvkArg0(i,v1) => specIMV(i,m,v1)
1 reachableI(i), specIMV(i,m,v1), VH(v1,a) => IM(i,m)
1 reachableI(i), statIM(i,m) => IM(i,m)
1 IinvkArg(i,z,u), IM(i,m), MmethArg(m,z,v1) => VV(v1,u)
1 IinvkRet(i,z,u), IM(i,m), MmethRet(m,z,v1) => VV(u,v1)
1 VV(u,v1), VH(v1,h), VHfilter(u,h) => VH(u,h)
1 reachableM(m), MobjValAsgnInst(m,l,h) => objValAsgnInst(l,h)
1 reachableM(m), MobjVarAsgnInst(m,l,r) => objVarAsgnInst(l,r)
1 reachableM(m), MgetInstFldInst(m,l,b,f) => getInstFldInst(l,b,f)
1 reachableM(m), MputInstFldInst(m,b,f,r) => putInstFldInst(b,f,r)
1 reachableM(m), MgetStatFldInst(m,l,f) => getStatFldInst(l,f)
1 reachableM(m), MputStatFldInst(m,f,r) => putStatFldInst(f,r)
1 reachableI(i), conNewInstIH(i,h), HT(h,t) => reachableT(t)
1 reachableI(i), objNewInstIH(i,h), HT(h,t) => reachableT(t)
1 reachableI(i), clsForNameIT(i,t) => reachableT(t)
1 reachableI(i), objNewInstIM(i,m) => reachableM(m)
1 reachableI(i), conNewInstIM(i,m) => reachableM(m)
1 reachableI(i), objNewInstIM(i,m), MmethArg(m,0,v1), objNewInstIH(i,h), VHfilter(v1,h) => VH(v1,h)
1 reachableI(i), objNewInstIH(i,h), IinvkRet(i,0,v1), VHfilter(v1,h) => VH(v1,h)
1 reachableI(i), conNewInstIM(i,m), IinvkArg(i,1,u), MmethArg(m,1,v1), VH(u,h), HFH(h,0,h2), VHfilter(v1,h2) => VH(v1,h2)
1 reachableI(i), conNewInstIM(i,m), MmethArg(m,0,v1), conNewInstIH(i,h), VHfilter(v1,h) => VH(v1,h)
1 reachableI(i), conNewInstIH(i,h), IinvkRet(i,0,v1), VHfilter(v1,h) => VH(v1,h)
1 reachableI(i), aryNewInstIH(i,h), IinvkRet(i,0,v1), VHfilter(v1,h) => VH(v1,h)
1 objValAsgnInst(l,h), VHfilter(l,h) => VH(l,h)
1 objVarAsgnInst(l,r), VH(r,h), VHfilter(l,h) => VH(l,h)
1 VH_33_1(l,h2), VHfilter(l,h2) => VH(l,h2)
1 HFH_34_0(f,r,h1), VH(r,h2) => HFH(h1,f,h2)
1 getStatFldInst(l,f), FH(f,h), VHfilter(l,h) => VH(l,h)
1 putStatFldInst(f,r), VH(r,h) => FH(f,h)
1 objValAsgnInst(a,h), HT(h,t) => reachableT(t)
1 reachableM(m), staticTM(t,m) => reachableT(t)
1 putStatFldInst(f,a), staticTF(t,f) => reachableT(t)
1 getStatFldInst(a,f), staticTF(t,f) => reachableT(t)
1 classT(t), reachableT(s), sub(s,t) => reachableT(t)
rootM(0).
1 reachableT(t), clinitTM(t,m) => rootM(m)
1 IM(i,n), MI(m,i) => MM(m,n)
1 getInstFldInst(l,b,f), VH(b,h1) => VH_33_0(l,f,h1)
1 VH_33_0(l,f,h1), HFH(h1,f,h2) => VH_33_1(l,h2)
1 putInstFldInst(b,f,r), VH(b,h1) => HFH_34_0(f,r,h1)
