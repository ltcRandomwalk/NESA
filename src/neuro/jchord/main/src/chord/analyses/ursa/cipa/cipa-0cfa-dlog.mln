*dom_T(t_dom)
*dom_H(h_dom)
*dom_M(m_dom)
*dom_V(v_dom)
*dom_Z(z_dom)
*dom_F(f_dom)
// input predicates
*staticTM(t_dom, m_dom)
*MgetInstFldInst(m_dom, v_dom, v_dom, f_dom)
*conNewInstIM(i_dom, m_dom)
*specIM(i_dom, m_dom)
*sub(t_dom, t_dom)
*staticTF(t_dom, f_dom)
*statIM(i_dom, m_dom)
